import java.util.Scanner;

public class SwapNum {
	public static void main(String[] args)
	{
		System.out.println("Enter two numbers for swaping");
		Scanner sc = new Scanner(System.in);
		int x = sc.nextInt();
		int y = sc.nextInt();
		System.out.println("Value of 1st number before swapping = "+x);
		System.out.println("Value of 2nd number before swapping = "+y);
		x=x+y;
		y=x-y;
		x=x-y;
		System.out.println("");
		System.out.println("Value of 1st number after swapping = "+x);
		System.out.println("Value of 2nd number after swapping = "+y);		
	    sc.close();
	}
}
